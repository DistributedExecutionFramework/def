<?import at.enfilo.def.local.simulator.gui.ClusterGuiComponent?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root type="at.enfilo.def.local.simulator.gui.ClusterGuiComponent" xmlns:fx="http://javafx.com/fxml/1">
    <children>
    <GridPane hgap="5.0" vgap="5.0" HBox.hgrow="ALWAYS">
        <children>
            <Label text="Cluster Id:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
            <TextField fx:id="txtClusterId" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="0"/>

            <Label text="Default MapRoutine Id:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
            <TextField fx:id="txtDefaultMapRoutineId" GridPane.columnIndex="1" GridPane.rowIndex="1"/>

            <Label text="StoreRoutine Id:" GridPane.columnIndex="0" GridPane.rowIndex="3"/>
            <TextField fx:id="txtStoreRoutineId" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
        </children>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
        <columnConstraints>
            <ColumnConstraints hgrow="NEVER" />
            <ColumnConstraints hgrow="ALWAYS"/>
        </columnConstraints>
    </GridPane>
    </children>
</fx:root>
