<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.Pane?>
<HBox xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.enfilo.def.local.simulator.gui.SimulatorGuiController">
    <children>
        <VBox spacing="5.0" HBox.hgrow="NEVER">
            <children>
                <Label text="Programs/Jobs/Tasks:" />
                <TreeView fx:id="trvProgramsJobsTasks" onMouseReleased="#trvReleased" BorderPane.alignment="CENTER" VBox.vgrow="ALWAYS" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </VBox>
        <Separator orientation="VERTICAL" HBox.hgrow="NEVER" />
        <VBox fx:id="pnlSelection" HBox.hgrow="ALWAYS" />
    </children>
</HBox>
